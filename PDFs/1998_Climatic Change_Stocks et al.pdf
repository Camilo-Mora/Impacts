        bindLoadButtonHandle(button);
        bindClickButtonHandle(button);
    });

    var sublinks = document.getElementsByClassName('js-buybox__inst-sub-link');
    forEach(sublinks, function (index, link) {
        bindClickLinkHandle(link);
    });
})();
