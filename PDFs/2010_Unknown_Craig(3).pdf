    function trackOnLoad(trackingConfig) {
        if (loadEventSent) return;
        loadEventSent = true;
        if (!trackingConfig) return;
        trackingConfig.sendinfo();
    }

    function trackSubscription(trackingConfig) {
        if (!trackingConfig) return;
        trackingConfig.sendinfo({linkId: 'inst. subscription info'});
