    function Track(product, webtrekkTrackId, eCommerceParam_11, action) {
        if (!product) return;

        var data = {
            trackDomain: trackDomain,
            trackId: webtrekkTrackId,
            domain: domain,
            contentId: 'SL-article',
            product: product.id,
            productStatus: action,
            productCategory: {
                1: product.trackingCategory
            },
            customEcommerceParameter: {
                9: domain,
