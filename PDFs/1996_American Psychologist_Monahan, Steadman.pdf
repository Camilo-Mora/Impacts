<script language="javascript">

	
		activeRemoteRequestCount = 0;
		comboPassAlreadyInCartHtml = '<div id="obArticleButton"><a id="comboIsInCartLink" href="/index.cfm?fa=buy.cart">VIEW CART</a>Combo&nbsp;Pass&nbsp;in&nbsp;cart</div>';
		doChangeDayPassHtml = false;
		itemAddedIsComboItem = false;
		optionToBuyPreviousPage = "";		
		passAlreadyInCartHtml = '<div id="obArticleButton"><a id="isInCartLink" href="/index.cfm?fa=buy.cart">VIEW CART</a>This item is already in your cart.</div>';
		passJustAddedToCart = false;
		shoppingCartLink = "/index.cfm?fa=buy.cart";
	

	//	when the page loads we check to see if the item, pass and/or combo pass are already in the cart
	jQuery(document).ready(function($) {	
		
			apaItemCode = '2700001';
			comboApaItemCode = '4840001';
			itemUID = '1996-05971-005';
	
			
	
				//	if item is in cart
				if(itemIsInCart(apaItemCode, itemUID)){
					alreadyInCartHtml = jQuery('#obArticleButtonAlreadyInCart').html();					
					jQuery('#obArticleButton').html(alreadyInCartHtml);					
					jQuery("#purchaseLink").removeClass("addToCartButton addingItemToCart").addClass("itemInCart");
				}	
			
			
		
	});	
	
	//	checks to see if the item is already in the cart...used by jQuery.ready (above)
	function itemIsInCart(apaItemCode, itemUID){
		
		//	can call Shantanu's isInCart() function, but we'll use ours for now
		//	returnVar = isInCart(itemUID);
		
			returnVar = false;
			
			if(apaItemCode == '4840001'){
				returnVar = false;
			}
		

		return returnVar;
	}
	
	//	fires when a user clicks 'add to cart'
	function purchaseItem(apaItemCode, itemUID){	
		
		var shoppingCartLink = '#variables.shoppingCartLink#';
		
		if(! arguments[1]){
			itemUID = '0';
		}
		
		
		
		/*		Validate PsycINFO, PsycEXTRA and PsycCRITIQUES day passes are added to ShoppingCart. 
				If so, convert it into Combo day pass. 
		*/
		var hasPI=false
		var hasPE=false
		var hasPQ=false
	
		// PsycCRITIQUES 2020001
		if(apaItemCode == '2020001'){
			if(hasPI && hasPE){
				apaItemCode='4840001';
			}
		} // PsycINFO 3500001
		else if(apaItemCode == '3500001'){
			if(hasPQ && hasPE){
				apaItemCode='4840001';
			}
		}else{
			if(hasPI && hasPQ){
				apaItemCode='4840001';
			}
		}
		
		shoppingCartLink = '/index.cfm?fa=buy.cart';
		productCode = 'PA_Document';
		if(apaItemCode == '4840001'){
			productCode = 'DayPassCombo';				
		}
			
			
				//	combo day pass
				if(apaItemCode == '4840001'){
					name = 'Day Pass Combo';
					imageUrl = 'http://psycnet.apa.org/images/db_triple_gray.gif';
					productUrl = '';					
					isOfpPurchase = false;
					
					itemAddedIsComboItem = true;
					doChangeDayPassHtml = true;	
					
					//	if the day pass is already in the cart, remove it!
					//	if(false == true){
					//	}					

					//	remove all day passes from cart
					removeDayPassesFromCart();
										
				//	item
				}else{
					name = "Violent storms and violent people: How meteorology can inform risk communication in mental health law.";
					var description = 'Monahan, John; Steadman, Henry J.<br />American Psychologist,  Vol 51(9), Sep   1996, 931-938.';
					var imageUrl = 'http://www.apa.org/pubs/journals/images/amp-100.gif';
					var productUrl = 'http://psycnet.apa.org/journals/amp/51/9/931';

					//	if the item being added is a day pass, indicate it in the constant
					if(itemUID == '0'){
						passJustAddedToCart = true;
					}
				
				}
				documentCode = 'amp';
				isOfpPurchase = 'false';
				sessionId = '';
			
			
			activeRemoteRequestCount = activeRemoteRequestCount + 1;
			addPsycnetProductToCart
				(
				sessionId, 
				apaItemCode, 
				itemUID, 
				name, 
				description, 
				imageUrl, 
				productUrl, 
				documentCode, 
				isOfpPurchase, 
				onItemAddedToCart
				);
		
	}

	function removeDayPassesFromCart(){
		activeRemoteRequestCount = activeRemoteRequestCount + 3;
		removeProduct('3700001', onItemRemovedFromCart);
		removeProduct('3500001', onItemRemovedFromCart);
		removeProduct('2020001', onItemRemovedFromCart);
	}
	
	function onItemRemovedFromCart(response){

		activeRemoteRequestCount = activeRemoteRequestCount - 1;

		/*
		response.status		means
		0					success
		1					failure
		*/
		
		if(passJustAddedToCart == true){
			jQuery('#passButtonContainer').html(comboPassAlreadyInCartHtml);
		}
		if(activeRemoteRequestCount < 1){
			onAllRemoteRequestsEnd();
		}
	}
	
	function onAllRemoteRequestsEnd(){
		
		//	update cart count
		activeRemoteRequestCount = activeRemoteRequestCount + 1;
		refreshCart();	//TODO: need function name from Shantanu
		activeRemoteRequestCount = activeRemoteRequestCount - 1;
	}
	
	function onItemAddedToCart(response) {

		activeRemoteRequestCount = activeRemoteRequestCount - 1;
		
		/*
		0: Success
		1: User is not logged in (this is used to purchase items for which user need to be logged in)
		2: User is not authorized to buy this product (something like a  member only product)
		3: Product not found in DB
		4: User already have active license and cannot purchase this (this is currently strictly for CE items)
		5: Quantity requested not available
		6: User not allowed to buy more than one at a time
		7: CESAS related (overlook this)
		*/
		cartResponse = response.addToCartResponse;
		if(cartResponse == 0){
			updateShoppingCart(response.quantity, 0);
			
			if(doChangeDayPassHtml == true){
				jQuery('#passButtonContainer').html(comboPassAlreadyInCartHtml);
			}
			
			if(itemAddedIsComboItem){			

				updateComboPurchaseLink(shoppingCartLink);
				updateComboPriceDiv();
			}else{
				updatePurchaseLink(shoppingCartLink);
				updatePriceDiv();
			}
				showDialog('purchaseConfirmationDialog');

		}else{
			showDialog('alreadyPurchasedDialog');
		}
		
		if(activeRemoteRequestCount < 1){
			onAllRemoteRequestsEnd();
		}
	}

	//	hides all dialogues and redirects users back to the previous page
	function continueShopping(){
		jQuery('#purchaseConfirmationDialog').hide();
		jQuery('#alreadyPurchasedDialog').hide();
		window.location.replace(optionToBuyPreviousPage);
	}
	
	//	sends users to checkout
	function goToCheckout(theLink){
		window.location.replace(theLink);
	}
	
	function updateComboPriceDiv(){
		jQuery("#theComboPrice").html(jQuery('#addedToCartLocale').val());	
	}
	
	function updateComboPurchaseLink(){
		jQuery("#comboPurchaseLink").html(jQuery('#viewCartLocale').val());	
		jQuery("#comboPurchaseLink").attr("href", shoppingCartLink);		
		jQuery("#comboPurchaseLink,#comboIsInCartLink").removeClass("addToCartButton addingItemToCart").addClass("itemInCart");
	}
	
	function updatePriceDiv(){
		jQuery("#thePrice").html(jQuery('#addedToCartLocale').val());	
	}
	
	function updatePurchaseLink(){
		jQuery("#purchaseLink").html(jQuery('#viewCartLocale').val());	
		jQuery("#purchaseLink").attr("href", shoppingCartLink);
		jQuery("#purchaseLink").removeClass("addToCartButton addingItemToCart").addClass("itemInCart");
	}	
	
	function showAddingToCart(targetID){	
		if(jQuery("#"+targetID).hasClass("addItemToCart")){
			jQuery("#"+targetID).removeClass("addItemToCart").addClass("addingItemToCart").html(jQuery('#adddingToCartLocale').val());
		}				
	}
	
</script>

